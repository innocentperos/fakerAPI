  },
  "dependencies": {
    "@faker-js/faker": "^7.5.0",
    "express": "^4.18.2"
  },
  "repository": {
    "type":"git",
    "url":"https://github.com/innocentperos/fakerAPI.git"
  },
  "bugs": {
